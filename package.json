    "mysql2": "^1.5.3",
    "passport": "^0.4.0",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-router-dom": "^4.3.1",
    "react-scripts": "1.1.4",
    "reactstrap": "^6.1.0",
    "sequelize": "^4.38.0",
    "sequelize-cli": "^4.0.0",
    "axios": "^0.16.2",
